   <top>
      <ButtonBar buttonMinWidth="0.0" maxHeight="-Infinity" maxWidth="700.0" prefHeight="40.0" prefWidth="630.0" BorderPane.alignment="TOP_LEFT">
        <buttons>
            <Button mnemonicParsing="false" onAction="#handleBooks" text="Master Books Report" />
            <Button mnemonicParsing="false" onAction="#handleLimits" text="Member Limits and Fines" />
            <Button mnemonicParsing="false" onAction="#handleWeeklyBooks" text="Weekly Books and Fines Report" />
            <Line endX="100.0" opacity="0.0" startX="40.0" />
        </buttons>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ButtonBar>
   </top>
</BorderPane>
